{
  "crons": [
    {
      "path": "/api/cron/reset-spins",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/send-sms",
      "schedule": "0 12 * * *"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  }
} 